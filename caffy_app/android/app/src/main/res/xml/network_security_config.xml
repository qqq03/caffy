<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- 개발용: 모든 HTTP 허용 (프로덕션에서는 HTTPS 사용 권장) -->
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
</network-security-config>
